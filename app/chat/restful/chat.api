syntax = "v1"

type Message {
	From        uint64 `json:"from,optional"`
	To          uint64 `json:"to"`
	Type        uint8  `json:"type"`
	ContentType uint8  `json:"contentType"`
	Time        string `json:"time"`
	Content     string `json:"content"`
}

@server (
	prefix: /app
)
service chat {
	@doc "聊天"
	@handler chatWS
	get /chat
}

