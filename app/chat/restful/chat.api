syntax = "v1"

type Message {
	Id          string `json:"id"`
	From        uint64 `json:"from,optional"`
	To          uint64 `json:"to"`
	Type        uint32 `json:"type"`
	ContentType uint32 `json:"contentType"`
	Time        string `json:"time"`
	Content     string `json:"content"`
}

type Transmit {
	Ip string
	Message
}

type Success {
	Id string `json:"id"`
}

@server (
	prefix: /app
)
service chat {
	@doc "聊天"
	@handler chatWS
	get /chat

}

